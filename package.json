{
	"name": "mp-starter-vscode-ext",
	"displayName": "MicroProfile Starter",
	"description": "VS Code extension starter for Eclipse MicroProfile",
	"version": "0.0.1",
	"publisher": "IBM",
	"preview": true,
	"license": "EPL-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/dev-tools-for-enterprise-java/mp-starter-vscode-ext"
	},
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/microprofile-logo.png",
	"activationEvents": [
		"onCommand:extension.microProfileStarter"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.microProfileStarter",
				"title": "MicroProfile Starter"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"extract-zip": "^1.6.7"
	}
}
